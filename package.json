        "angular:angular"
      ]
    },
    "angulartics-mixpanel": {
      "depends": [
        "angular:angular",
        "angulartics:angulartics"
      ]
    },
    "angular-bootstrap": {
      "depends": [
        "angular:angular"
      ]
    }
  },
  "main": "server/index.js",
