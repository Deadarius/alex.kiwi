{
  "name": "alex.kiwi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./server/index.js"
  },
  "dependencies": {
    "angular": "^1.3.15",
    "angular-bootstrap": "^0.12.0",
    "angulartics": "^0.17.2",
    "bootstrap": "^3.3.4",
    "cacher": "^2.1.1",
    "express": "^4.12.3",
    "lodash": "^3.6.0",
    "marked": "^0.3.3",
    "mixpanel-data-export": "^1.6.2",
    "morgan": "^1.5.2",
    "node-lessify": "0.0.10",
    "request-promise": "^0.4.2"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "browserify-shim": "^3.8.5",
    "coffee-script": "^1.9.1",
    "del": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-angular-templatecache": "^1.6.0",
    "gulp-html-replace": "^1.4.5",
    "gulp-nodemon": "^2.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.2.0",
    "run-sequence": "^1.0.2",
    "vinyl-source-stream": "^1.1.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "node-lessify"
    ]
  },
  "browser": {
    "angular": "./node_modules/angular/angular.js",
    "angulartics": "./node_modules/angulartics/src/angulartics.js",
    "angulartics-mixpanel": "./node_modules/angulartics/src/angulartics-mixpanel.js",
